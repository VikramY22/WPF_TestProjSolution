\pard\sa200\sl276\slmult1\b0\fs22\'c4\'eb\'ff \'e2\'f2\'ee\'f0\'ee\'e3\'ee \'e7\'e0\'ef\'f0\'ee\'f1\'e0 \'e1\'f3\'e4\'e5\'ec \'e8\'f1\'ef\'ee\'eb\'fc\'e7\'ee\'e2\'e0\'f2\'fc \'e2\'ed\'e5\'f8\'ed\'e8\'e9 \'ea\'eb\'fe\'f7 \'e4\'eb\'ff \'f1\'ee\'e7\'e4\'e0\'ed\'e8\'ff \'f1\'f1\'fb\'eb\'ea\'e8 \'ed\'e0 \'ea\'ee\'ed\'ea\'f0\'e5\'f2\'ed\'fb\'e9 \'ea\'ee\'ed\'f2\'e5\'e9\'ed\'e5\'f0.\line\'cf\'ee\'f1\'ea\'ee\'eb\'fc\'ea\'f3 \'ee\'ed \'f1\'ee\'e7\'e4\'e0\'ed \'e2 \'f2\'e5\'f1\'f2\'ee\'e2\'fb\'f5 \'e7\'e0\'ef\'f0\'ee\'f1\'e0\'f5, \'e5\'ec\'f3 \'f3\'e6\'e5 \'ef\'f0\'e8\'f1\'e2\'ee\'e5\'ed \f2\lang1033 ID\f1\lang1049 ==1  - \'f1\'ee\'f8\'eb\'e5\'ec\'f1\'ff \'ed\'e0 \'ed\'e5\'e3\'ee.\line\'c4\'eb\'ff \'fd\'f2\'ee\'e3\'ee \'ed\'f3\'e6\'ed\'ee \'f1\'e4\'e5\'eb\'e0\'f2\'fc \'ef\'ee\'f1\'eb\'e5, \'ea\'ee\'f2\'ee\'f0\'ee\'e5 \'e4\'e5\'ea\'eb\'e0\'f0\'e8\'f0\'f3\'e5\'f2 \'e2\'ed\'e5\'f8\'ed\'e8\'e9 \'ea\'eb\'fe\'f7 \'e8 \'ef\'f0\'e8\'f1\'e2\'ee\'e8\'f2\'fc \'e5\'ec\'f3 \'ed\'f3\'e6\'ed\'ee\'e5 \'e7\'ed\'e0\'f7\'e5\'ed\'e8\'e5 \'e8\'e7 \'f2\'e0\'e1\'eb\'e8\'f6\'fb \'ea\'ee\'ed\'f2\'e5\'e9\'ed\'e5\'f0\'ee\'e2.\line\line\b DECLARE @ContainerID BIGINT = 1(\'f3\'ea\'e0\'e7\'fb\'e2\'e0\'e5\'f2\'f1\'ff \f2\lang1033 ID\f1\lang1049  \'ed\'f3\'e6\'ed\'ee\'e3\'ee \'ea\'ee\'ed\'f2\'e5\'e9\'ed\'e5\'f0\'e0); \fs32\par
\b0\fs22\'ce\'f1\'f2\'e0\'eb\'fc\'ed\'ee\'e5 \'ef\'e8\'f8\'e5\'f2\'f1\'ff \'ef\'ee \'ef\'ee\'e4\'ee\'e1\'e8\'fe \'ef\'e5\'f0\'e2\'ee\'e3\'ee \'e7\'e0\'ef\'f0\'ee\'f1\'e0, \'f7\'e5\'f0\'e5\'e7 \'ea\'ee\'ed\'ea\'e0\'f2\'e5\'ed\'e0\'f6\'e8\'fe (\f2\lang1033 STRING_AGG)\b\f1\fs32\lang1049\par

\pard\sa200\sl276\slmult1\qc\par

\pard\sa200\sl276\slmult1\b0\fs22 DECLARE @ContainerID BIGINT = 1;\par
SELECT \par
    '[' + STRING_AGG(\par
        '\{' +\par
            '"ID":' + CAST(ID AS VARCHAR) + ',' +\par
            '"ContainerID":' + CAST(ContainerID AS VARCHAR) + ',' +\par
            '"OperationStartDate":' + '"' + CONVERT(VARCHAR, OperationStartDate, 126) + '",' +\par
            '"OperationEndDate":' + '"' + CONVERT(VARCHAR, OperationEndDate, 126) + '",' +\par
            '"OperationType":' + '"' + ISNULL(OperationType, '') + '",' +\par
            '"OperatorFullName":' + '"' + ISNULL(OperatorFullName, '') + '",' +\par
            '"InspectionPlace":' + '"' + ISNULL(InspectionPlace, '') + '"' +\par
        '\}'\par
    , ',') + ']' AS JsonOperations\par
FROM Operations\par
WHERE ContainerID = @ContainerID;\b\fs32\par

